	<Medium 	feedrate="400" power="255" repeat="1" hexcode="#0000ff" tool="laser"/>
	<Dark 		feedrate="200" power="255" repeat="1" hexcode="#00ff00" tool="laser"/>
	<Score 		feedrate="800" power="255" repeat="10" hexcode="#000000" dwell="20" tool="laser"/>
